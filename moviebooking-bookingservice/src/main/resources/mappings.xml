<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings
	xmlns="http://java.sun.com/xml/ns/persistence/orm"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm orm_2_0.xsd"
	version="2.0">
	<entity class="com.shrey.moviebooking.commons.model.Booking">
		<table name="bookings" />
		<schema name="bookingdb" />
		<attributes>
			<id name="id">
				<generated-value strategy="IDENTITY" />
			</id>
			<column name="showId" />
			<column name="theatreId" />
			<column name="screenId" />
			<column name="userid" />
			<column name="amount" />
			<column name="status" />
			<column name="created" />
			<column name="updated" />
		</attributes>
	</entity>
	<entity class="com.shrey.moviebooking.commons.model.BookedSeat">
		<table name="bookedseats" />
		<schema name="bookingdb" />
		<attributes>
			<id name="id">
				<generated-value strategy="IDENTITY" />
			</id>
			<column name="bookingId" />
			<column name="seatId" />
			<column name="created" />
			<column name="updated" />
		</attributes>
	</entity>
</entity-mappings>