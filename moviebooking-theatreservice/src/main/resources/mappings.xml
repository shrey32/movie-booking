<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings
	xmlns="http://java.sun.com/xml/ns/persistence/orm"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm orm_2_0.xsd"
	version="2.0">
	<entity class="com.shrey.moviebooking.commons.model.Theatre">
		<table name="theatres" />
		<schema name="theatresdb" />
		<attributes>
			<id name="id">
				<generated-value strategy="IDENTITY" />
			</id>
			<column name="name" />
			<column name="addressId" />
			<column name="status" />
			<column name="created"></column>
			<column name="updated"></column>
		</attributes>
	</entity>
	<entity class="com.shrey.moviebooking.commons.model.Address">
		<table name="addresses" />
		<schema name="theatresdb" />
		<attributes>
			<id name="id">
				<generated-value strategy="IDENTITY" />
			</id>
			<column name="line1" />
			<column name="line2" />
			<column name="city" />
			<column name="state" />
			<column name="country" />
			<column name="pincode" />
			<column name="created"></column>
			<column name="updated"></column>
		</attributes>
	</entity>
	<entity class="com.shrey.moviebooking.commons.model.Contact">
		<table name="contacts" />
		<schema name="theatresdb" />
		<attributes>
			<id name="id">
				<generated-value strategy="IDENTITY" />
			</id>
			<column name="contactType" />
			<column name="contact" />
			<column name="created"></column>
			<column name="updated"></column>
		</attributes>
	</entity>
	<entity class="com.shrey.moviebooking.commons.model.TheatreContact">
		<table name="theatrecontact" />
		<schema name="theatresdb" />
		<attributes>
			<id name="id">
				<generated-value strategy="IDENTITY" />
			</id>
			<column name="theatreId" />
			<column name="contactId" />
			<column name="created"></column>
			<column name="updated"></column>
		</attributes>
	</entity>
	<entity class="com.shrey.moviebooking.commons.model.Screen">
		<table name="screens" />
		<schema name="theatresdb" />
		<attributes>
			<id name="id">
				<generated-value strategy="IDENTITY" />
			</id>
			<column name="name" />
			<column name="theatreId" />
			<column name="capacity" />
			<column name="created"></column>
			<column name="updated"></column>
		</attributes>
	</entity>
	<entity
		class="com.shrey.moviebooking.commons.model.Seat">
		<table name="seats" />
	<schema name="theatresdb" />
		<attributes>
			<id name="id">
				<generated-value strategy="IDENTITY" />
			</id>
			<column name="screenId" />
			<column name="theatreId" />
			<column name="rowName" />
			<column name="seatNumber" />
			<column name="created"></column>
			<column name="updated"></column>
		</attributes>
	</entity>
	<entity class="com.shrey.moviebooking.commons.model.Show">
		<table name="shows" />
		<schema name="theatresdb" />
		<attributes>
			<id name="id">
				<generated-value strategy="IDENTITY" />
			</id>
			<column name="theatreId" />
			<column name="screenId" />
			<column name="movieId" />
			<column name="startTime" />
			<column name="price"></column>
			<column name="status"></column>
			<column name="created"></column>
			<column name="updated"></column>
		</attributes>
	</entity>
</entity-mappings>